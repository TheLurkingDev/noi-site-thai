{
  "name": "backend",
  "version": "1.0.0",
  "description": "Backend server for dating website",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "postinstall": "npm run build",
    "init-db": "ts-node src/db/init.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "dating",
    "express",
    "typescript",
    "postgresql"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.8.7",
    "@types/pg": "^8.11.10",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "pg": "^8.13.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3"
  },
  "devDependencies": {
    "rimraf": "^5.0.0"
  }
}
